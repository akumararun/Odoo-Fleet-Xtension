	<record id="fleet_x_vehicle_form" model="ir.ui.view">
		<field name="name">fleet.vehicle.form.inherit</field>
		<field name="model">fleet.vehicle</field>
		<field name="inherit_id" ref="fleet.fleet_vehicle_form" />
		<field name="arch" type="xml">
			<field name="transmission" position="after">
				<field name="engine_type" />
				<field name="engine_power" />

			</field>
			<field name="color" position="after">
				<field name="fueltankcap" />
				<field name="manufacture_year" />
				<field name="ownership" />
			</field>
			<field name="acquisition_date" position="before">
				<field name="odometer_date" />
			</field>

			<field name="driver_id" position="after">
				<field name="department_id" options="{'no_create': True}" required="1"/>
			</field>
			
			<field name="location" position="replace">
				<field name="location" invisible="1"/>
				<field name="location_id" options="{'no_create': True}" required="1"/>
			</field>
			
			<field name="driver_id" position="before">
				<field name="type_id" options="{'no_create': True}" required="1"/>
			</field>

			<div class="oe_right oe_button_box" position="inside">
				<button name="%(open_fleet_vehicle_issue)d"
                    			context="{'search_default_vehicle_id': [active_id],'default_vehicle_id': active_id}"
                                type="action"
                                class="oe_stat_button"
                                icon="fa-bug"
                                >
                                <field name="issue_count" widget="statinfo" string="Issues"/>
                            </button>
                            
                   <button name="%(open_fleet_vehicle_gallery)d"
                    			context="{'search_default_vehicle_id': [active_id],'default_vehicle_id': active_id}"
                                type="action"
                                class="oe_stat_button"
                                icon="fa-picture-o"
                                >
                                <field name="gallery_count" widget="statinfo" string="Galleries"/>
                            </button>
				<button name="action_get_attachment_tree_view" class="oe_stat_button attachment_2"
					icon="fa-book" type="object">
					<field name="attachment_count" widget="statinfo" string="Documents" />
				</button>
			</div>
			
			<field name="acquisition_date" position="replace">
				<field name="acquisition_date" invisible="1"/>
			</field>
			<field name="car_value" position="replace">
				<field name="car_value" invisible="1"/>
			</field>
			
			<xpath expr="/form/sheet/group[position()=last()]" position="after">
                  <notebook>
                  	<page string="Statistics">
                            <group col="2"  string="Purchase Info">
                                <field name="ppartner" />
                                <field name="acquisition_date" required="1" />
                                <field name="car_value" />
                                <field name="podometer" required="1"/>
                                <label for="warrexp" string="Warranty"/>
                                <div>
                                    <field name="warrexp" class="oe_inline"/> (date) or
                                    <field name="warrexpmil" class="oe_inline"/> (distance)
                                </div>
                            </group>
                            <group col="2" >
                            	<group string="Cost Stats">
                            		<field name="costtotal" />
	                                <field name="costpmon" />
	                                <field name="costpm" />
	                                <field name="distance" />
	                                <field name="days" />
	                                <field name="lmiles" />
                            	</group>
                            	                                
                            </group>
                    	</page>
                    	<page string="Internal Note">
                    		<field name="note"/> 
                    	</page>
                    	<page string="Policies">
						</page>
                    </notebook>
              </xpath>

		</field>
	</record>

	
	<record id="fleet_x_vehicle_search" model="ir.ui.view">
		<field name="name">fleet.vehicle.search.inherit</field>
		<field name="model">fleet.vehicle</field>
