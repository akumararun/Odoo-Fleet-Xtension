<!--Overriding log contract filter
-->

       <record id="view_res_contract_filter" model="ir.ui.view">
            <field name="name">fleet.vehicle.log.contract.select</field>
            <field name="model">fleet.vehicle.log.contract</field>
            <field name="arch" type="xml">
                <search string="Search Contracts">
                   <field name="name"/> 
                    <field name="vehicle_id"/> 
                    <field name="insurer_id"/> 
                   <separator/>
                   <filter icon="terp-camera_test" domain="[('state','=','open')]" string="In-Process." name="open"/>
                   <filter icon="terp-camera_test" domain="[('state','=','closed')]" string="Closed" name="terminated"/>
                   <filter icon="terp-camera_test" domain="[('state','=','toclose')]" string="To Close" name="toclose"/>


                   <separator/>
                   <group expand="0" string="Group By">
                       <filter string="Vehicle" context="{'group_by': 'vehicle_id'}"/>
                       <filter string="Supplier" context="{'group_by': 'insurer_id'}"/>
                       <filter string="Type" context="{'group_by': 'cost_subtype_id'}"/>
                   </group>
               </search>
            </field>
        </record>

  
        <record model='ir.ui.view' id='fleet_vehicle_log_fuel_search'>
            <field name="name">fleet.vehicle.log.fuel.search</field>
            <field name="model">fleet.vehicle.log.fuel</field>
            <field name="inherit_id" ref="fleet.fleet_vehicle_log_fuel_search" />
            <field name="arch" type="xml">
                <field name="vehicle_id" position="after">
                    <field name="vendor_id" />
                </field>
                <group string="Group By" position="inside">
                    <filter name="groupby_vendor" context="{'group_by' : 'vendor_id'}" string="Vendor"/>
                </group>
            </field>
        </record>
        
        <record model='ir.ui.view' id='fleet_vehicle_log_services_search'>
            <field name="name">fleet.vehicle.log.services.search</field>
            <field name="model">fleet.vehicle.log.services</field>
            <field name="inherit_id" ref="fleet.fleet_vehicle_log_services_search" />
            <field name="arch" type="xml">
               <field name="vehicle_id" position="after">
                    <field name="vendor_id" />
                </field>
                <field name="cost_subtype_id" position="after">
                    <group expand="0" string="Group By">
                       <filter name="groupby_vendor" context="{'group_by' : 'vendor_id'}" string="Vendor"/>
                       <filter name="groupby_vehicle" context="{'group_by' : 'vehicle_id'}" string="Vehicle"/>
                   </group>
                    
                </field>
            </field>
        </record>

	<record id="fleet_x_vehicle_cost_tree" model="ir.ui.view">
		<field name="name">fleet.vehicle.cost.tree.inherit</field>
		<field name="model">fleet.vehicle.cost</field>
		<field name="inherit_id" ref="fleet.fleet_vehicle_cost_tree" />
		<field name="arch" type="xml">
			<field name="date" position="after">
				<field name="amount" />
			</field>
		</field>
	</record>
	
		<record model='ir.ui.view' id='fleet_vehicle_costs_search_inherit'>
			<field name="name">fleet.vehicle.cost.search_report</field>
			<field name="model">fleet.vehicle.cost</field>
			<field name="inherit_id" ref="fleet.fleet_vehicle_costs_search" />
			<field name="arch" type="xml">

				<xpath expr="//filter[@string='Vehicle']" position="after">
					<filter name="groupby_type_id"
						context="{'group_by': 'type_id'}"
						string="Vehicle Type" />

				</xpath>

			</field>
		</record>
		


	</data>
</openerp>

